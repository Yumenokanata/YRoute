<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="indi.yume.tools.yroute.sample">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme"
            android:name=".App"
            android:fullBackupContent="true">
        <activity android:name=".MainActivity"
                  android:configChanges="orientation|screenSize"
                  android:screenOrientation="unspecified">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />

                <action android:name="NEWS_DETAIL" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>

            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
            </intent-filter>
        </activity>
        <activity android:name=".normal.FragmentStackActivity"/>
        <activity android:name=".normal.OtherActivity"/>
        <activity android:name=".normal.SingleStackActivity"/>
        <activity android:name=".normal.NormalMainActivity" />

        <activity android:name=".fragmentmanager.FragmentStackActivity"/>
        <activity android:name=".fragmentmanager.OtherActivity" />
        <activity android:name=".fragmentmanager.SingleStackActivity"/>
        <activity android:name=".fragmentmanager.SupportMainActivity" />
    </application>

</manifest>